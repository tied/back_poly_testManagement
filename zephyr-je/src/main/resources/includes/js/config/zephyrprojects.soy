/**
 * @param reindexProjectList
 */
{template .listReindexSelectedProjects autoescape="true"}
	<div class="project-view-test-disable">
    	<div>
	    	<table class="aui" id="project-view-reindex-detail" >
			    <thead>
			        <tr>
			            <th>{getText('zephyr.admin.menu.globalsettings.config.projects.name')}</th>
			            <th>{getText('zephyr.admin.menu.globalsettings.config.projects.selected')}</th>
			        </tr>
			    </thead>
			    <tbody>
			    	{foreach $tdProject in $reindexProjectList}
			    		<tr data-projectid={$tdProject.properties.value} data-projectName={$tdProject.properties.label}>
				    	<td width="60%"><a href="{contextPath}/browse/{$tdProject.properties.key}">{$tdProject.properties.label}</a></td>
				    	<td width="30%">
							<input title="Please uncheck to not reindex this project." id={$tdProject.properties.value} type="checkbox" class="checkbox" style="vertical-align:baseline" checked="checked">
				    	</td>
					    </tr>
		    		{/foreach}
			    <tbody>
			</table>
        </div>
    </div>
{/template}